#!/usr/bin/expect -f
log_user 1
set timeout -1

cd ~/rl-swarm
spawn bash run_rl_swarm.sh

expect -re {Would you like to push models.*Hub\?} {
    send "n\r"
}

expect -re {Enter the name of the model.*default model\.} {
    send "\r"
}

expect -re {Would you like your model to participate.*Prediction Market\?} {
    send "n\r"
}

interact
